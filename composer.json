{
  "name": "fakrulhasan/smart-debugger",
  "description": "A Laravel package for advanced error debugging with Bengali AI suggestions and auto patch.",
  "keywords": ["laravel", "debugger", "bangla", "error", "patch"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fakrul Hasan Sajib",
      "email": "ahmedsajib1999s@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "FakrulHasan\\SmartDebugger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FakrulHasan\\SmartDebugger\\SmartDebuggerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
