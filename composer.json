{
    "name": "fakrulhasansajib/smart-debugger",
    "description": "A smart Laravel debugger with AI (Bangla), live notifications, and auto patch suggestions.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "FakrulHasanSajib\\SmartDebugger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FakrulHasanSajib\\SmartDebugger\\SmartDebuggerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
