{
  "name": "fakrulhasan/my-laravel-project",
  "description": "A Laravel project using Smart Debugger package",
  "type": "project",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "fakrulhasan/smart-debugger": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/FakrulHasanSajib/smart-debugger.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  }
}
