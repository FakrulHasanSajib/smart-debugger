{
    "name": "fakrulhasansajib/smart-debugger",
    "description": "A smart Laravel debugger with AI (Bangla), live notifications, and auto patch suggestions.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SmartDebugger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartDebugger\\SmartDebuggerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0",
        "openai-php/laravel": "^0.7.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
